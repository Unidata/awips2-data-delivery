    <!-- 
        This is an incremental override file, indicating that the files
        at different localization levels will be combined. 
    -->
<ParameterRegexes>
    <!-- List of patterns to match to the beginning of the description and removed. -->
    <!-- Note: Search is in order, so longer or more specific patterns should go first. -->
    <levelRegex order="1.0" id="mbLevels" pattern="\((\d+ )+(\d+..)( \d+)+\) " level="Pressure Levels"/> <!--MB -->
    <levelRegex order="2.0" id="hybridLevel" pattern="(\d+) hybrid level(( - \d+) hybrid level)? " level="Height Level" levelGroup="$1$3" units="hybrid level"/>  <!--Height (hybrid level -->
    <levelRegex order="3.0" id="sequence" pattern="(\d+) in sequence none " level="Sequence" levelGroup="$1"/>  <!-- -->
    <levelRegex order="4.0" id="kLevel" pattern="(\d+) k (isentropic )?level " level="Height Level" levelGroup="$1" units="k level"/>  <!-- Height (k level) -->
    <levelRegex order="5.0" id="aboveMeanSeaLevel" pattern="(\d+) m above mean sea level " level="Height Level" levelGroup="$1" units="m above sea level"/>  <!-- Height (m) -->
    <levelRegex order="6.0" id="mUnderground" pattern="(\d+) m underground " level="Height Level" levelGroup="-$1" units="m"/>  <!-- Height (-m) -->
    <levelRegex order="7.0" id="sigmaLevel" pattern="(\d+(\.\d+)?) sigma level " level="Sigma Level" levelGroup="$1" units="Kelvin"/>  <!-- SIGL-->
    <levelRegex order="8.0" id="belowGroundMB" pattern="(\d+(\.\d+)?(-\d+(\.\d+)?)?) mb below ground " level="Height Level" levelGroup="-$1" units="mb"/>  <!--MB-->
    <levelRegex order="9.0" id="aboveGroundMB" pattern="(\d+(\.\d+)?(-\d+(\.\d+)?)?) mb( above ground)? " level="Height Level" levelGroup="$1" units="mb"/>  <!--MB -->
    <levelRegex order="10.0" id="belowGround" pattern="(\d+(\.\d+)?(-\d+(\.\d+)?)?) m below ground " level="Height Level" levelGroup="-$1" units="m"/>  <!-- Height (-m)-->
    <levelRegex order="11.0" id="aboveGround" pattern="(\d+(\.\d+)?(-\d+(\.\d+)?)?) m( above ground)? " level="Height Level" levelGroup="$1" units="m"/>  <!-- Height (m) -->
    <levelRegex order="12.0" id="sigmaLayer" pattern="(\d+(\.\d+)?-\d+(\.\d+)?) sigma layer " level="Sigma Level" levelGroup="$1" units="Kelvin"/>  <!-- Sigma Level-->
    <levelRegex order="13.0" id="cIsotherm" pattern="\d+c isotherm " level="0c isotherm"/>  <!--FRZ -->
    <levelRegex order="14.0" id="cloudLevel" pattern="((boundary layer|grid scale) )?cloud (base|ceiling|top|bottom|layer)( level)? " level="Cloud Level"/>  <!--CBL -->
    <levelRegex order="15.0" id="highCloudLevel" pattern="high cloud (bottom|top|layer)( level)? " level="High Cloud Level"/>  <!-- HCY-->
    <levelRegex order="16.0" id="lowCloudLevel" pattern="low cloud (bottom|top|layer)( level)? " level="Low Cloud Level"/>  <!-- LCY-->
    <levelRegex order="17.0" id="middleCloudLevel" pattern="middle cloud (bottom|top|layer)( level)? " level="Middle Cloud Level"/>  <!-- MCY-->
    <levelRegex order="18.0" id="convectivCloudLevels" pattern="((deep|shallow) )?convective cloud (bottom|top|layer)( level)? " level="Convective Level" levelGroup='$1$3'/>  <!--CCL-->
    <levelRegex order="19.0" id="entireAtmosphere" pattern="entire atmosphere( \(considered as a single layer\))? " level="Entire Atmosphere (As Single Layer)"/>  <!--EA -->
    <levelRegex order="20.0" id="entireOcean" pattern="entire ocean( \(considered as a single layer\))? " level="Entire Ocean (As Single Layer)"/>  <!--  -->
    <levelRegex order="21.0" id="equilibriumLevel" pattern="equilibrium level " level="Equlibrium Level"/>  <!--EL -->
    <levelRegex order="22.0" id="highSupercooledWater" pattern="highest top level of supercooled liquid water layer " level="Highest Level"/>  <!--HSCLW -->
    <levelRegex order="23.0" id="lowSupercooledWater" pattern="lowest bottom level of supercooled liquid water layer " level="Lowest Level"/>  <!--LSCLW -->
    <levelRegex order="24.0" id="troposphericFreezingLevel" pattern="highest tropospheric freezing level " level="0c isotherm" units='highest tropospheric'/>  <!-- FRZ-->
    <levelRegex order="25.0" id="adiabaticCondensation" pattern="level of adiabatic condensation from sfc " level="Height Level"/>  <!-- Height -->
    <levelRegex order="26.0" id="wetBulbZero" pattern="lowest level of the wet bulb zero " level="Height Level"/>  <!-- -->
    <levelRegex order="27.0" id="maxWind" pattern="max wind " level="Maximum Level"/>  <!--MAXW -->
    <levelRegex order="28.0" id="meanSeaLevel" pattern="mean sea level " level="Mean Sea Level"/>  <!--MSL -->
    <levelRegex order="29.0" id="planetaryBoundary" pattern="planetary boundary layer " level="Planetary Boundary Layer"/>  <!--PBL -->
    <levelRegex order="30.0" id="pv" pattern="pv=(-?\d+(\.\d+)?e-?\d+) \(km\^2\/kg\/s\)( surface)? " level="PV Level" levelGroup="$1" units="km^2/kg/s"/>  <!--PVL -->
    <levelRegex order="31.0" id="topOfAtmosphere" pattern="top of atmosphere " level="Top Level"/>  <!-- CTL-->
    <levelRegex order="32.0" id="tropopause" pattern="tropopause " level="Tropopause Level"/>  <!--TROP -->
    <levelRegex order="33.0" id="surface" pattern="surface( none)? " level="Surface"/>  <!-- SFC -->

