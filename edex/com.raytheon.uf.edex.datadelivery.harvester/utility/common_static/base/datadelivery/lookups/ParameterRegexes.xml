    <!-- 
        This is an incremental override file, indicating that the files
        at different localization levels will be combined. 
    -->
<ParameterRegexes>
    <!-- List of patterns to match to the beginning of the description and removed. -->
    <!-- Note: Search is in order, so longer or more specific patterns should go first. -->
    <levelRegex order="1.0" id="numbers" pattern="\((\d+ )+(\d+..)( \d+)+\) "/>
    <levelRegex order="2.0" id="hybridLevel" pattern="\d+ hybrid level (- \d+ hybrid level)? "/>
    <levelRegex order="3.0" id="sequence" pattern="\d+ in sequence none "/>
    <levelRegex order="4.0" id="kLevel" pattern="\d+ k (isentropic )?level "/>
    <levelRegex order="5.0" id="aboveMeanSeaLevel" pattern="\d+ m above mean sea level "/>
    <levelRegex order="6.0" id="mUnderground" pattern="\d+ m underground "/>
    <levelRegex order="7.0" id="sigmaLevel" pattern="\d+(\.\d+)? sigma level "/>
    <levelRegex order="8.0" id="belowGround" pattern="\d+(\.\d+)?(-\d+(\.\d+)?)? m(b)? below ground "/>
    <levelRegex order="9.0" id="aboveGround" pattern="\d+(\.\d+)?(-\d+(\.\d+)?)? m(b)?( above ground)? "/>
    <levelRegex order="10.0" id="sigmaLayer" pattern="\d+(\.\d+)?-\d+(\.\d+)? sigma layer "/>
    <levelRegex order="11.0" id="cIsotherm" pattern="\d+c isotherm "/>
    <levelRegex order="12.0" id="boundaryLayerCloudLayer" pattern="boundary layer cloud layer "/>
    <levelRegex order="13.0" id="cloudLevels" pattern="(((deep)|(grid scale)|(high)|(low)|(middle)|(shallow)) )?(convective )?cloud ((bottom)|(top)|(base)|(ceiling)|(layer))( level)? "/>
    <levelRegex order="14.0" id="entireAtmosphere" pattern="entire atmosphere( \(considered as a single layer\)) "/>
    <levelRegex order="15.0" id="entireOcean" pattern="entire ocean( \(considered as a single layer\)) "/>
    <levelRegex order="16.0" id="equilibriumLevel" pattern="equilibrium level "/>
    <levelRegex order="17.0" id="supercooledWater" pattern="((highest top)|(lowest bottom)) level of supercooled liquid water layer "/>
    <levelRegex order="18.0" id="troposphericFreezingLevel" pattern="highest tropospheric freezing level "/>
    <levelRegex order="19.0" id="adiabaticCondensation" pattern="level of adiabatic condensation from sfc "/>
    <levelRegex order="20.0" id="wetBulbZero" pattern="lowest level of the wet bulb zero "/>
    <levelRegex order="21.0" id="maxWind" pattern="max wind "/>
    <levelRegex order="22.0" id="meanSeaLevel" pattern="mean sea level "/>
    <levelRegex order="23.0" id="planetaryBoundary" pattern="planetary boundary layer "/>
    <levelRegex order="24.0" id="pv" pattern="pv=-?\d+(\.\d+)?e-?\d+ \(km\^2\/kg\/s\) "/>
    <levelRegex order="25.0" id="topOfAtmosphere" pattern="top of atmosphere "/>
    <levelRegex order="26.0" id="tropopause" pattern="tropopause "/>
    <levelRegex order="27.0" id="surface" pattern="surface( none)? "/>

