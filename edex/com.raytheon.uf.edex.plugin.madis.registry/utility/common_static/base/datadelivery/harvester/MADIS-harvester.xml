<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
    <!-- 
        This is an absolute override file, indicating that a higher priority 
        version of the file will completely replace a lower priority version
        of the file. 
    -->
<harvester>
    <provider serviceType="WFS" name="MADISOGC">
        <connection>
            <!-- for OGC it's your FQDN -->
            <url>http://${harvester.external.address}:8085/wfs</url>
            <userName>user</userName>
            <password>password</password>
            <encryption>CLEAR</encryption>
        </connection>
        <providerType dataType="POINT" plugin="madis" availabilityDelay="0" />
        <projection type="LatLon">
            <name>MadisLatLon</name>
            <description>MADIS Test LatLon Coverage</description>
        </projection>
    </provider>
    <!-- MADIS doesn't purge Data Set Meta Data -->
    <retention>-1</retention>
    <agent xsi:type="ogcAgent" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <dateFormat>HHddMMMyyyy</dateFormat>
    </agent>
</harvester>