<!--
    Sample urlParserInfo describing the various options.
    
    <urlParserInfo ignore="true" name="test"> // ignore: skip crawling this urlParserInfo (default: false)
                                              // name: name of the urlParserInfo
        <seedUrl>test</seedUrl>               // seedUrl: Base link in the URL after the provider URL
        <urlKey>test</urlKey>                 // urlKey: Section of URL link applied after seedUrl. 
                                              //         Usually in the form of 'urlKey+date'.
        <dateFormat>yyMMdd</dateFormat>       // dateFormat: Format of the dates in the URL
                                              //             (default: yyyyMMdd)
        // pattern: allows for processing of unusually formed URLs. Splits the section of the URL using
        //          the provided 'regex'. Can specify the index containing the 'dataSet' and 'cycle'.
        <pattern dataSetLocation="0" cycleLocation="1" regex="\." />
        // dataSetNaming: Some urlParserInfo are for multiple datasets, so this allows the alternative 
        //                naming conventions to be specified.
        //    alternate 1 - 'collectionName+dataSetName': 
        //       Joins the collectionName and datasetName using the specified separator.
        //    alternate 2 - 'collectionName': 
        //       Names the dataSet after the collection name. This will merge data from different
        //       collections into a single dataset.
        <dataSetNaming separator="_" expression="collectionName+dataSetName" />
    </urlParserInfo>
-->
